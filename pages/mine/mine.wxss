.headView {
  width: 100%;
  height: 320rpx;
  background-color: #F9DA34;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.logo_button {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  /* position: fixed; */
  /* margin-left: 240rpx; */
  margin: 0rpx;
  padding: 0rpx;
  background-color: #F9DA34; 
  line-height:1.5;
  margin-top: 15rpx
}
.logo_button:after {
  border: 0px;
  /* background-color: #F9DA34; */
}

.logoimg {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  border: 20rpx solid #FCE777;
  margin-bottom: 10px;
}
.headiew_title {
  font-size: 12pt;
  color: black;
}
.midleview {
  height: 200rpx;
  width: 100%;
  margin-top: 20rpx;
  background-color: #FFF;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.midleview_container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center; 
}
.midleview_container_count {
  font-size: 25px;
  color: #666666;
  text-align:center;
  margin-bottom: 20rpx;
}
.midleview_container_title {
  font-size: 13px;
  color: #A8A8A8;
  text-align: center;
}
.text_color {
  color: #FED90C;
}
.bottomview {
  /* background-color: #FFF; */
  margin-top: 10px;
  padding-bottom:100rpx;
  background-color: #f9f9f9;
}
.bottomview_cell {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-content: center;
  height: 100rpx;
  border-bottom: 1px solid #E5E5E5; 
  background-color: #FFF;
}
.left_cell {
  display: flex;
  /* flex-direction: column; */
  justify-content: center;
  /* margin-top: 40px;  */
  align-items: center; 
}
.icon {
  padding-left:44rpx; 
  padding-right: 20rpx;
  width: 16px;
  height: 18px;
}
.icon_0 {
  padding-left:44rpx; 
  padding-right: 28rpx;
  width: 27rpx;
  height: 34rpx;
}
.icon_1 {
  padding-left:44rpx; 
  padding-right: 20rpx;
  width: 35rpx;
  height: 35rpx;
}
.icon_2 {
  padding-left:44rpx; 
  padding-right: 21rpx;
  width: 32rpx;
  height: 36rpx;
}
.icon_3 {
  padding-left:44rpx; 
  padding-right: 19rpx;
  width: 34rpx;
  height: 25rpx;
}
.icon_4 {
  padding-left:44rpx; 
  padding-right: 14rpx;
  width: 40rpx;
  height: 31rpx;
}
.titleLabel {
  color: #333;
  font-size: 12pt;
  font-weight: bold;
}
.right_cell {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
}
.nextIcon {
  padding-right: 40rpx;
  width: 14rpx;
  height: 24rpx;
}
.cs_button {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-content: center;
  height: 100rpx;
  border-bottom: 1px solid #E5E5E5; 
  background-color: #FFF;
  /* background-color: #fff; */
  border: 0px;
  width: 100%;
  margin: 0rpx;
  padding: 0rpx;
  /* height: auto; */
  /* display: flex; */
  /* justify-content: space-between; */
  /* align-content: center; */
}
.cs_button:after {
  border: 0px;
}
